lug\r\n  const changeSlug = (e) => {\r\n    setSlug(e.target.value.trim().replace(/ /g, \"-\").toLowerCase());\r\n  };\r\n\r\n  const onSubmit = async (data) => {\r\n    saveArticle(data,slug,file,history)\r\n  };\r\n\r\n  return (\r\n    // form for new article\r\n    <div className=\"container pr-5\">\r\n      <div className=\"tabla\">\r\n        <div className=\"row mb-4\">\r\n          <h2 className=\"text-center text-secondary col align-self-center\">\r\n            New Article\r\n          </h2>\r\n        </div>\r\n\r\n        {/* form here */}\r\n        <form onSubmit={handleSubmit(onSubmit)} enctype=\"multipart/form-data\">\r\n          <div className=\"row mr-2\">\r\n            <div className=\"col-lg-8 col-md-12\">\r\n              <div className=\"row\">\r\n                <div className=\"col\">\r\n                  <div className=\"mb-3\">\r\n                    <label htmlFor=\"title\" className=\"form-label\">\r\n                      Article name\r\n                    </label>\r\n                    <input\r\n                      type=\"text\"\r\n                      onChange={changeSlug}\r\n                      className=\"form-control\"\r\n                      autoComplete=\"off\"\r\n                      name=\"title\"\r\n                      aria-describedby=\"emailHelp\"\r\n                      ref={register({\r\n                        required: \"Title is required\",\r\n                      })}\r\n                    />\r\n                    <p className=\"text-danger\">{errors.title?.message}</p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div className=\"row\">\r\n                <div className=\"col\">\r\n                  <div className=\"mb-3\">\r\n                    <label htmlFor=\"slug\" className=\"form-label\">\r\n                      Slug\r\n                    </label>\r\n                    <input\r\n                      type=\"text\"\r\n                      className=\"form-control\"\r\n                      disabled\r\n                      value={slug}\r\n                      name=\"slug\"\r\n                      aria-describedby=\"emailHelp\"\r\n                    />\r\n                  </div>\r\n                </div>\r\n                <div className=\"col\">\r\n                  <div className=\"mb-3\">\r\n                    <label htmlFor=\"title\" className=\"form-label\">\r\n                      Category\r\n                    </label>\r\n                    <select\r\n                      className=\"form-control\"\r\n                      name=\"category_id\"\r\n                      ref={register({\r\n                        required: \"Choose a category\",\r\n                      })}\r\n                    >\r\n                      \r\n                      {categories.length !== 0 &&\r\n                        categories.map((category) => (\r\n                          <option value={category.id} key={category.id}>\r\n                            {category.name}\r\n                          </option>\r\n                        ))}\r\n                    </select>\r\n                    <p className=\"text-danger\">{errors.category_id?.message}</p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n\r\n              <div className=\"row\">\r\n                <div className=\"col\">\r\n                  <div className=\"mb-3\">\r\n                    <label htmlFor=\"slug\" className=\"form-label\">\r\n                      Short text\r\n                    </label>\r\n                    <input\r\n                      type=\"text\"\r\n                      className=\"form-control\"\r\n                      autoComplete=\"off\"\r\n                      name=\"short_text\"\r\n                      aria-describedby=\"emailHelp\"\r\n                      ref={register({\r\n                        required: \"Short text is required\",\r\n                      })}\r\n          